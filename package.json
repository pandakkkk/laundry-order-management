    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.0.0",
    "morgan": "^1.10.0",
    "pdfkit": "^0.17.2",
    "qrcode": "^1.5.4",
    "twilio": "^5.11.1"
  },
  "devDependencies": {
    "baseline-browser-mapping": "^2.9.11",
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.1"
  }
}
